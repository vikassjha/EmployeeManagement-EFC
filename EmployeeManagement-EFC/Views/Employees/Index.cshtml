@model List<EmployeeManagement_EFC.Models.Domains.Employee>


<h1> List of Employees</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Salary</th>
            <th>Date of Birth</th>
            <th>Department</th>
            <th>Options</th>
        </tr>

    </thead>
    <tbody>

        @foreach (var employee in Model)
        {
            <tr>

                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Email</td>
                <td>@employee.Salary</td>
                <td>@employee.DateOfBirth.ToString()</td>
                <td>@employee.Department</td>
                <td><a href="/Employees/Details/@employee.Id">View</a></td>





            </tr>
        }
    </tbody>

</table>

